{
    "name": "Yumikoo",
    "description": "A Telegram Management + Music Player Bot, written in Python with Pyrogram and Py-Tgcalls.",
    "logo": "https://telegra.ph/file/dbb3222d0fd3712e2a0e4.jpg",
    "keywords": [
    "python3",
    "telegram",
    "bot",
    "DAXX",
    "MusicBot",
    "telegram-bot",
    "pyrogram"
    ],
 "repository": "https://github.com/Mohitag24/Yumikoo",
      "stack": "heroku-22",
      "addons": [
    {
       "options": {
          "version": "12"
       },
       "plan": "heroku-postgresql"
    }
 ],
    "env": {
      "API_ID": {
        "description": "Get this value from https://my.telegram.org",
        "value": "",
        "required": true
      },
      "API_HASH": {
        "description": "Get this value from https://my.telegram.org",
        "value": "",
        "required": true
      },
      "BOT_TOKEN": {
        "description": "A Bot's token from Botfather",
        "value": "",
        "required": true
      },
      "MONGO_URL": {
        "description": "Get a mongodb url from https://cloud.mongodb.com.",
        "value": "",
        "required": true
      },
      "OWNER_ID": {
        "description": "The user id of user whom you would like to add as OWNER.",
        "value": "",
        "required": true
      },
      "SESSION_STRING": {
        "description": "A Pyrogram v2 String Session from @StringFatherBot on Telegram.",
        "value": "",
        "required": true
      },
      "BOT_USERNAME":{
         "value": "",
         "required": true
      },
      "COMMAND_HANDLER":{
         "value": "",
         "required": true
      },
      "SUDO_USERS":{
         "value": "",
         "required": true
      },
    },
    "buildpacks": [
          {
              "url": "heroku/python"
          },
          {
              "url": "heroku/nodejs"
          },
          {
              "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
          }
    ]
 }
